{
  "name": "@rn-lalala/core",
  "version": "0.0.5",
  "main": "dist/commonjs/index.js",
  "module": "dist/module/index.js",
  "react-native": "src/index.ts",
  "types": "dist/typescript/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/typescript/index.d.ts",
      "import": "./dist/module/index.js",
      "require": "./dist/commonjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "react-component"
  ],
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@rn-lalala/hello-world": "workspace:^0.0.8",
    "@rn-lalala/hello-world2": "workspace:^0.0.8"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "react": "18.2.0",
    "react-native": "0.72.0"
  },
  "peerDependencies": {
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "react-native": ">=0.71.7"
  },
  "scripts": {
    "build": "bob build",
    "typecheck": "tsc --noEmit"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
